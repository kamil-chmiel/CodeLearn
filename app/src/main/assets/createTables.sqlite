CREATE TABLE IF NOT EXISTS ranking ( id varchar(30) PRIMARY KEY, points int )
CREATE TABLE IF NOT EXISTS users ( id varchar(30) PRIMARY KEY, email varchar(30), name varchar(30) )
CREATE TABLE IF NOT EXISTS friend_list ( id varchar(30), friend_id varchar(30) )
CREATE TABLE IF NOT EXISTS lectures ( id varchar(30) PRIMARY KEY, filename varchar(30))
CREATE TABLE IF NOT EXISTS quizzes ( id varchar(30) PRIMARY KEY, filename varchar(30))
CREATE TABLE IF NOT EXISTS user_lectures ( user_id varchar(30) PRIMARY KEY, last_lecture_id varchar(30))
CREATE TABLE IF NOT EXISTS user_quizzes ( unique_id varchar(30) PRIMARY KEY, user_id varchar(30) , quiz_id varchar(30), score int)